{
  "name": "antd",
  "version": "0.10.5",
  "stableVersion": "0.10.5",
  "title": "Ant Design",
  "description": "一个 UI 设计语言",
  "homepage": "http://ant.design/",
  "keywords": [
    "ant",
    "design",
    "react",
    "react-component",
    "component",
    "components",
    "ui",
    "framework",
    "frontend"
  ],
  "contributors": [
    "ant"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ant-design/ant-design"
  },
  "bugs": {
    "url": "https://github.com/ant-design/ant-design/issues"
  },
  "main": "lib/index",
  "files": [
    "lib",
    "style"
  ],
  "license": "MIT",
  "dependencies": {
    "classnames": "~2.2.0",
    "css-animation": "~1.5.0",
    "gregorian-calendar": "~4.1.6",
    "gregorian-calendar-format": "~4.1.3",
    "object-assign": "~4.1.1",
    "rc-animate": "~2.6.0",
    "rc-calendar": "~9.10.10",
    "rc-checkbox": "~2.1.6",
    "rc-collapse": "~1.11.1",
    "rc-dialog": "~7.3.0",
    "rc-dropdown": "~2.4.1",
    "rc-form-validation": "~2.5.0",
    "rc-input-number": "~4.4.0",
    "rc-menu": "~7.4.21",
    "rc-notification": "~3.3.1",
    "rc-pagination": "~1.17.8",
    "rc-progress": "~2.3.0",
    "rc-queue-anim": "~1.6.12",
    "rc-radio": "~2.0.0",
    "rc-select": "~9.0.0",
    "rc-slider": "~8.6.6",
    "rc-steps": "~3.3.1",
    "rc-switch": "~1.9.0",
    "rc-table": "~6.4.3",
    "rc-tabs": "~9.6.1",
    "rc-time-picker": "~3.6.2",
    "rc-tooltip": "~3.7.3",
    "rc-tree": "~1.14.10",
    "rc-trigger": "~2.6.2",
    "rc-upload": "~2.6.3",
    "rc-util": "~4.6.0",
    "react-slick": "~0.23.2",
    "reqwest": "~2.0.5",
    "semver": "~5.6.0",
    "util-deprecate": "~1.0.1",
    "velocity-animate": "~1.5.2",
    "warning": "~4.0.3"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "busboy": "^0.3.0",
    "chalk": "^2.4.2",
    "clipboard": "^2.0.4",
    "css-loader": "^2.1.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-markdown": "git+https://github.com/ant-design/eslint-plugin-markdown.git",
    "eslint-plugin-react": "^7.12.4",
    "expect.js": "~0.3.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "gh-pages": "^2.0.1",
    "history": "^4.7.2",
    "jest-cli": "~24.1.0",
    "json-loader": "^0.5.1",
    "less": "~3.9.0",
    "less-loader": "^4.1.0",
    "lesshint": "git+https://github.com/ant-design/lesshint.git",
    "lodash": "^4.17.11",
    "nico-jsx": "~0.9.0",
    "pre-commit": "1.x",
    "react": "~16.8.3",
    "react-addons-test-utils": "~15.6.2",
    "react-dom": "~16.8.3",
    "react-router": "~4.3.1",
    "webpack": "^4.29.5",
    "webpack-babel-jest": "^1.0.0",
    "webpack-dev-middleware": "^3.6.0"
  },
  "scripts": {
    "babel": "babel components index.js --out-dir lib",
    "start": "npm run clean && nico server --watch",
    "clean": "rm -rf _site dist",
    "deploy": "rm -rf node_modules && node scripts/install.js && npm run just-deploy",
    "just-deploy": "npm run clean && webpack --config webpack.deploy.config.js && webpack --config webpack.antd.config.js && NODE_ENV=PRODUCTION nico build",
    "lint": "eslint components test index.js --ext '.js,.jsx' && npm run mdlint && npm run lesshint",
    "mdlint": "eslint components/*/demo/*.md --ext '.md' --global 'React,ReactDOM' --rule 'no-console: 0'",
    "lesshint": "lesshint style/ -e 'style/+(core|mixins)/+(base|iconfont|normalize|layouts|compatibility|grid).less'",
    "test": "npm run lint && webpack && npm run jest",
    "jest": "jest",
    "pub": "sh ./scripts/publish.sh",
    "webpack": "webpack",
    "beta": "sh ./scripts/publish.sh --tag beta"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/*"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/_site/"
    ],
    "scriptPreprocessor": "<rootDir>/node_modules/webpack-babel-jest",
    "testDirectoryName": "tests",
    "preprocessCachingDisabled": false
  },
  "pre-commit": [
    "lint"
  ]
}
